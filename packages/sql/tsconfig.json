{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@destacks/sql": [
        "./src/index"
      ],
      "@destacks/sql/react": [
        "./src/react"
      ]
    },
    "esModuleInterop": true,
    "target": "ES6",
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "strict": true,
    "outDir": "dist/",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "useDefineForClassFields": true,
    "module": "ESNext",
  },
  "include": [
    "**/*.ts",
  ],
  "exclude": [
    "node_modules/",
    "dist/"
  ]
}